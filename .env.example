# Environment Configuration
ENV=local

# Service Configuration
SERVICE_NAME=eduscale-engine
SERVICE_VERSION=0.1.0

# Google Cloud Platform Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=europe-west1
GCP_RUN_SERVICE=eduscale-engine

# Storage Configuration
STORAGE_BACKEND=local
GCS_BUCKET_NAME=

# Upload Constraints
MAX_UPLOAD_MB=50
ALLOWED_UPLOAD_MIME_TYPES=text/plain,text/markdown,text/csv,text/html,application/json,application/pdf,application/octet-stream,application/vnd.openxmlformats-officedocument.wordprocessingml.document,application/vnd.openxmlformats-officedocument.spreadsheetml.sheet,audio/mpeg,audio/mp4,audio/x-m4a,audio/m4a,audio/wav,audio/ogg,audio/webm,audio/flac

# MIME Decoder Service Configuration
TRANSFORMER_SERVICE_URL=
BACKEND_SERVICE_URL=
REQUEST_TIMEOUT=300
BACKEND_UPDATE_TIMEOUT=5
LOG_LEVEL=INFO
